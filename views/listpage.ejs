<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>Video Game Name</th>
            <th>URL of Video Game</th>
            <th>Created By</th>
            <th>Added By</th>
            <th>Likes</th>
            <th>Actions</th>
        </tr>

        <% items.forEach(function(item){  %>


<tr>
        <td>
    <%= item.videoGameName %></td>
        <td> <%= item.URLofvideogame %></td>
        <td> <%= item.Createdby %></td>
        <td> <%= item.addedBy %></td>
        <td> <%= item.Likes %></td>
        <td> <%= item.Actions %></td>

        </tr>


        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
